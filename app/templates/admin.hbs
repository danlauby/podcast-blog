<!-- Renders new-post-tile.hbs depending on if newPostFormShowing is true or false -->
{{new-post-tile savePost="savePost"}}

<!-- Renders update-delete.hbs for admin delete/update, passes through data updatePost and destroyPost to database -->
<ul>
  {{#each model as |post|}}
  {{update-delete post=post destroyPost="destroyPost"}}
  {{/each}}
</ul>
